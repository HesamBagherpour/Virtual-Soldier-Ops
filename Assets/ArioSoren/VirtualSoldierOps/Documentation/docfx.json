{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "ArioSoren.VirtualSoldierOps.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**"
          ],
          "src": "../../../../"
        }
      ],
      "globalNamespaceId": "Global",
      "filter": "filterConfig.yml",
      "properties": {
        "TargetFramework": "netstandard2.1"
      },
      "dest": "_api~"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.yml"
        ],
        "src": "_api~",
        "dest": "api~"
      },
      {
        "files": [
          "*.md",
          "toc.yml"
        ]
      },
      {
        "files": [
          "manual/**/*.{md,yml}",
          "license/**/*.{md,yml}",
          "change-log/**/*.{md,yml}",
          "third-party-notices/**/*.{md,yml}"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/images/**",
          "**/artifacts/**",
          "codesnippet/**",
          "langwordmapping/**"
        ],
        "exclude": [
          "_site/**",
          "**/obj/**",
          "**.meta"
        ]
      }
    ],
    "xref": [
      "https://normanderwan.github.io/UnityXrefMaps/xrefmap.yml"
    ],
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "postProcessors": [
      "ExtractSearchIndex"
    ],
    "globalMetadata": {
      "_appTitle": "ArioSoren Project",
      "_appFooter": "<span>Copyright Â© 2022 ArioSoren.com<br>Generated by <strong>DocFX</strong></span>",
      "_enableSearch": true,
      "_appLogoPath": "images/logo-ariosoren.png",
      "_appFaviconPath": "images/favicon.ico"
    },
    "dest": "_site~",
    "markdownEngineName": "markdig",
    "template": [
      "default",
      "modern",
      "custom"
    ]
  },
  "templateData": {
    "templateName": "ArioSoren Project Template",
    "version": "1.0.0"
  }
}